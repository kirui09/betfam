<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#CCCCCC" /> <!-- Change this color to match your background color -->
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="#FFFFFF" /> <!-- Change this color to white -->
            </shape>
        </clip>
    </item>
</layer-list>
